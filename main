void precarga(){
    int opcion, exito, DNI;
    Prestador LSO[110+1], p;
FILE *fp=fopen("Prestadores.txt", "r");
while (!feof(fp)){
fscanf(fp, "%d", &opcion);
    switch(opcion){
    case 1:
    fscanf(fp, "%d", &p.DNI);
    fscanf(fp, "%[^\n]", &p.nombreyapellido);
    fscanf(fp, "%[^\n]", &p.servicios);
    fscanf(fp, "%[^\n]", &p.domicilio);
    fscanf(fp, "%[^\n]", &p.correo);
    fscanf(fp, "%[^\n]", &p.telefono);
    LSO_Alta(LSO, p, &exito);
    break;
    case 2:    fscanf(fp, "%d", &DNI);
               LSO_Evocar(LSO, &p, DNI, &exito);
               break;
    case 3:
        fscanf(fp, "%d", &DNI);
        LSO_Baja(LSO, DNI, &exito);
    }
}
}
